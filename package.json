{
  "name": "synfhir-store",
  "version": "1.1.0",
  "description": "Moleculer, Postgres FHIR store created for Yorkshire and Humber Care Record",
  "scripts": {
    "linux:proxy:up": "cd run/linux && docker-compose up",
    "linux:proxy:up:detached": "cd run/linux run && docker-compose up -d",
    "linux:proxy:down":"cd run/linux && docker-compose down",
    "linux:proxy:start":"cd run/linux && docker-compose start",
    "linux:proxy:stop":"cd run/linux && docker-compose stop",
    "windows:proxy:up": "cd run/windows && docker-compose up",
    "windows:proxy:up:detached": "cd run/windows run && docker-compose up -d",
    "windows:proxy:down":"cd run/windows && docker-compose down",
    "windows:proxy:start":"cd run/windows && docker-compose start",
    "windows:proxy:stop":"cd run/windows && docker-compose stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synanetics/synfhir-store.git"
  },
  "author": "Synanetics Ltd.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synanetics/synfhir-store/issues"
  },
  "homepage": "https://github.com/synanetics/synfhir-store#readme"
}
