ADVANCED_CONSENT=false
# Maximum number of async queries to run concurrently
ASYNCMAXQUERIES=1
# How long to wait between checks (in seconds)
ASYNCPOLLPERIOD=10
ASYNCPAGESIZE=1000
ASYNCEXPIRES=7
AUDIT_ACCESS=false
CACHEEXPIRES=600
# FHIR STORE CONFIGURATION (for local development machines)
FHIR_STORE_BASE_URL=http://localhost:3001/fhir/stu3
FHIR_STORE_ASYNC_URL=http://localhost:3001/fhir/stu4/bulk
# PUBLIC CERTIFICATE (DEVELOPMENT) for validating JWTs - available at https://iam.development.yhcr.nhs.uk/pki/iam
IAM_CERTIFICATE=-----BEGIN CERTIFICATE----------END CERTIFICATE-----
LOGGER=true
LOGLEVEL=debug
NAMESPACE=synfhir-fhirstore
# Post gres connection pooling - can be changed
MAX_CLIENTS=1
# Maximum data page size in search resukts
MAXPAGESIZE=1000
METRICSENABLE=false
NODE_ENV=debug

PORT=3000
# Moleculer microservice configuration... which services are required
SERVICEDIR=services
# Replase [ODS-CODE] and [ORGANISATION-NAME] with your Trust's respective values
TAG={"system":"https://yhcr.nhs.uk/Source","code":"[ODS-CODE]","display":"[ORGANISATION-NAME]"}